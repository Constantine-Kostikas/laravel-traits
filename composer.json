{
    "name": "gpapakitsos/laravel-traits",
    "description": "A bundle of some useful Laravel Model/Controller traits",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Papakitsos",
            "email": "papakitsos_george@yahoo.gr",
            "homepage": "https://papakitsos.gr"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "nesbot/carbon": "^2.62"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "GPapakitsos\\LaravelTraits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GPapakitsos\\LaravelTraits\\Tests\\": "tests/",
            "Database\\Factories\\": "tests/database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GPapakitsos\\LaravelTraits\\TraitsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
